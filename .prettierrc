{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"jsxSingleQuote": false,
	"bracketSpacing": false,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "lf",
	"proseWrap": "preserve",
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"printWidth": 80
			}
		}
	]
}
